{% extends 'BackendBundle::backend.html.twig' %}

{% block body %}
    {{ parent() }}
    <h1>Secondcategory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ secondCategory.id }}</td>
            </tr>
            <tr>
                <th>Lib</th>
                <td>{{ secondCategory.lib }}</td>
            </tr>
            <tr>
                <th>Div_Name</th>
                <td>{{ secondCategory.divName }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('secondcategory_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('secondcategory_edit', { 'id': secondCategory.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
