{% extends 'BackendBundle:Layout:backend.html.twig' %}

{% block body %}
    {{ parent() }}
    <h1>Globalorder</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ globalOrder.id }}</td>
            </tr>
            <tr>
                <th>Dateorder</th>
                <td>{% if globalOrder.dateOrder %}{{ globalOrder.dateOrder|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('globalorder_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('globalorder_edit', { 'id': globalOrder.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
